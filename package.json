{
  "name": "vidoc",
  "displayName": "vidoc",
  "description": "vidoc",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vidoc.helloWorld",
        "title": "[Vidoc] Hello World"
      },
      {
        "command": "vidoc.readConfig",
        "title": "[Vidoc] Read Config"
      },
      {
        "command": "vidoc.startRecording",
        "title": "[Vidoc] Start Recording"
      },
      {
        "command": "vidoc.stopRecording",
        "title": "[Vidoc] Stop Recording"
      },
      {
        "command": "vidoc.wininfo",
        "title": "[Vidoc] wininfo"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "@arcsine/win-info": "^0.2.13",
    "@types/lodash": "^4.14.195",
    "@types/uuid": "^9.0.2",
    "lodash": "^4.17.21",
    "record-screen": "^3.19.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.8.0",
    "uuid": "^9.0.0",
    "yauzl": "^2.10.0"
  }
}
